@using BillingSystem.Models






















































@model BidDetailsView

@{
    ViewBag.Title = "BidDetails View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style type="text/css">
    .home {
        -webkit-box-shadow: 10px 10px 1px 0px;
        margin: 4px 31px 62px 41px;
        border: 6px solid #E6E6FA;
        box-shadow: 10px 10px 1px 0px;
        width: 150px;
        height: 65px;
        float: left;
        text-align: center;
        background-color: #5c7499;
    }
</style>
<script src="~/Scripts/BidDetails/BidDetails.js"></script>

<div class="white-bg">
    <div class="panel panel-default collapsecss ourcollapse">
        <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseBidDetailsAddEdit">
            <h4 class="panel-title collapseTitle">
                <span class="glyphicon glyphicon-plus-sign"></span>Add BidDetails
                <a class="btn btn-default btn-sm pull-right collapse-btn">
                    <span class="glyphicon glyphicon-chevron-down"></span>
                </a>
            </h4>
        </div>
        <div id="collapscollapseBidDetailsAddEditeOne" class="panel-collapse collapse">
            <div class="panel-body">
                <div class="base-color">
                    <div class="form">                        
                        @{Html.RenderPartial("~/Views/BidDetails/UserControls/_BidDetailsAddEdit.cshtml", Model.CurrentBidDetails);}
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel panel-default collapsecss ourcollapse bordertable">
        <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseBidDetailsList">
            <h4 class="panel-title collapseTitle">
                <span><img src="../images/list.png" /></span>BidDetails List
                <a class="btn btn-default btn-sm pull-right collapse-btn">
                    <span class="glyphicon glyphicon-chevron-down"></span>
                </a>
            </h4>
        </div>
        <div id="collapseBidDetailsList" class="panel-collapse collapse in tableImg">
            <div class="panel-body">
                <div class="base-color" id="BidDetailsListDiv">
                    @{Html.RenderPartial("~/Views/BidDetails/UserControls/_BidDetailsList.cshtml", Model.BidDetailsList);}
                </div>
            </div>
        </div>
    </div>
    <div class="clear"></div>
</div>
            
